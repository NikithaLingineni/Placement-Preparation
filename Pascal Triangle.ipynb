    "def Pascal(n):\n",
    "    triangle=[]\n",
    "    for i in range(n):\n",
    "        triangle.append(getrow(i))\n",
    "    return triangle\n",
    "def getrow(rowindex):\n",
    "    ans=1\n",
    "    res=[1]\n",
    "    for i in range(rowindex):\n",
    "        ans=ans*(rowindex-i)\n",
    "        ans=ans/(i+1)\n",
    "        res.append(int(ans))\n",
    "    return res\n",
    "n=int"
   ]
  }
